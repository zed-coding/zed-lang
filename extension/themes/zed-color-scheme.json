      {
          "scope": "entity.name.function.call.zed",
          "settings": {
              "foreground": "#DCDCAA"
          }
      },
      {
          "scope": "variable.parameter.zed",
          "settings": {
              "foreground": "#9CDCFE"
          }
      },
      {
          "scope": "variable.other.zed",
          "settings": {
              "foreground": "#9CDCFE"
          }
      },
      {
          "scope": "support.type.std.zed",
          "settings": {
              "foreground": "#4EC9B0"
          }
      },
      {
          "scope": "entity.name.namespace.zed",
          "settings": {
              "foreground": "#4EC9B0"
          }
      },
      {
          "scope": "string.include.path.zed",
          "settings": {
              "foreground": "#CE9178"
          }
      },
      {
          "scope": "variable.other.register.asm.zed",
          "settings": {
              "foreground": "#9CDCFE"
          }
