      {
          "scope": "support.function.asm.instruction.zed",
          "settings": {
              "foreground": "#569CD6"
          }
      },
      {
          "scope": "constant.numeric.asm.zed",
          "settings": {
              "foreground": "#B5CEA8"
          }
      },
      {
          "scope": ["keyword.operator.assignment.zed", "keyword.operator.arithmetic.zed", "keyword.operator.logical.zed", "keyword.operator.comparison.zed"],
          "settings": {
              "foreground": "#D4D4D4"
          }
      },
      {
          "scope": "punctuation.separator.zed",
          "settings": {
              "foreground": "#D4D4D4"
          }
      }
  ]
}
