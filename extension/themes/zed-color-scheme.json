{
  "name": "Zed Theme",
  "tokenColors": [
      {
          "scope": ["comment.line.double-slash.zed", "comment.block.zed", "comment.line.number-sign.asm.zed"],
          "settings": {
              "foreground": "#608B4E"
          }
      },
      {
          "scope": "keyword.other.directive.zed",
          "settings": {
              "foreground": "#C586C0"
          }
      },
      {
          "scope": "keyword.control.zed",
          "settings": {
              "foreground": "#C586C0"
          }
      },
      {
          "scope": "keyword.control.flow.zed",
          "settings": {
              "foreground": "#C586C0"
          }
      },
      {
          "scope": "keyword.declaration.function.zed",
          "settings": {
              "foreground": "#C586C0"
          }
      },
      {
          "scope": "string.quoted.double.zed",
          "settings": {
              "foreground": "#CE9178"
          }
      },
      {
          "scope": "constant.character.escape.zed",
          "settings": {
              "foreground": "#D7BA7D"
          }
      },
      {
          "scope": ["constant.numeric.decimal.zed", "constant.numeric.hex.zed"],
          "settings": {
              "foreground": "#B5CEA8"
          }
